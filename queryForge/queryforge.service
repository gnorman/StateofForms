[Unit]
Description=QueryForge Docker Services
After=docker.service
Requires=docker.service

[Service]
Type=forking
RemainAfterExit=yes
WorkingDirectory=/home/gnorm/Documents/Projects/StateofForms/queryForge
ExecStart=/usr/bin/docker compose up -d
ExecStop=/usr/bin/docker compose down
TimeoutStartSec=0
User=gnorm
Group=gnorm

[Install]
WantedBy=multi-user.target